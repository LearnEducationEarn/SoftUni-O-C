function maxNumber(input) {
    let maxNum = Number.NEGATIVE_INFINITY;

    while (input.length > 0) {
        let command = input.shift();

        if (command === "Stop") {
            break;    
        }
        let number = Number(command);

        if (number > maxNum) {
            maxNum = number;
        }
}
        console.log(maxNum);
}
