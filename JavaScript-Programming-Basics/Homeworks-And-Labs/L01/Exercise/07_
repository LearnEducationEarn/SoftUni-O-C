function foodDelivery(input) {
    const CHICKEN_MENU_PRICE = 10.35;
    const FISH_MENU_PRICE = 12.40;
    const VEGETARIAN_MENU_PRICE = 8.15;

    let countChickenMenu = Number(input[0]);
    let countFishMenu = Number(input[1]);
    let countVegetarianMenu = Number(input[2]);


    let totalChickenCosts = countChickenMenu * CHICKEN_MENU_PRICE;
    let totaFishCosts = countFishMenu * FISH_MENU_PRICE;
    let totalVegetarianCosts = countVegetarianMenu * VEGETARIAN_MENU_PRICE;

    let totalForMenu = totalChickenCosts + totaFishCosts + totalVegetarianCosts

    let desertPrice = totalForMenu * 0.20;

    let result = totalForMenu + desertPrice + 2.50;

    console.log(result);


}
