function fishTank(input) {
    let length = Number(input[0]);
    let width = Number(input[1]);
    let height = Number(input[2]);
    let percent = Number(input[3]);

    //Един литър вода се равнява на един кубичен дециметър/ 1л= 1 дм3/.
    let tankVolume = length * width * height;  //Обема в см^3
    let littersVolume = tankVolume / 1000;     //Обема в литри   1л = 1 дм^3
    let occupiedSpace =  percent / 100;             //заето пространство
    let neededLitters = littersVolume * (1 - occupiedSpace);


    console.log(neededLitters);


}
